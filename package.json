    "@nestjs-modules/ioredis": "^1.0.0",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.2.5",
    "@nestjs/core": "^8.2.5",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/platform-express": "^8.2.5",
    "@nestjs/platform-socket.io": "^8.2.5",
    "@nestjs/schedule": "^1.0.2",
