  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "schema:fresh": "mikro-orm schema:fresh --run",
    "schema:update": "mikro-orm schema:update --run",
    "schema:drop": "mikro-orm schema:drop --run"
  },
  "dependencies": {
    "@mikro-orm/core": "^5.1.2",
    "@mikro-orm/nestjs": "^5.0.1",
    "@mikro-orm/postgresql": "^5.1.2",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs-modules/ioredis": "^1.0.0",
    "@nestjs/bull": "^0.5.4",
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/platform-socket.io": "^8.4.4",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/websockets": "^8.4.4",
    "apify": "^2.3.0",
    "apollo-server-express": "^3.6.7",
    "bull": "^4.8.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "envalid": "^7.3.1",
    "ghost-cursor": "^1.1.10",
    "graphql": "^16.3.0",
    "graphql-type-json": "^0.3.2",
    "html-to-text": "^8.2.0",
    "ioredis": "^5.0.4",
    "lodash": "^4.17.21",
    "node-libcurl": "^2.3.4",
    "p-retry": "^5.1.0",
    "pg": "^8.7.3",
    "puppeteer": "^13.5.2",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "set-cookie-parser": "^2.4.8",
    "stream-to-promise": "^3.0.0",
    "undici": "^5.0.0",
    "user-agents": "^1.0.991",
    "xregexp": "^5.1.0"
