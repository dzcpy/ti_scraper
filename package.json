    "envalid": "^7.2.2",
    "ghost-cursor": "^1.1.8",
    "graphql": "^15.7.1",
    "graphql-type-json": "^0.3.2",
    "html-to-text": "^8.1.0",
    "ioredis": "^4.28.3",
    "lodash": "^4.17.21",
    "node-libcurl": "^2.3.4-1",
    "p-retry": "^5.0.0",
    "pg": "^8.7.1",
    "puppeteer": "^13.0.1",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "reflect-metadata": "^0.1.13",
